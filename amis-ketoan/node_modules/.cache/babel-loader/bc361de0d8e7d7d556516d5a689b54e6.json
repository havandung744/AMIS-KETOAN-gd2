{"ast":null,"code":"import { mapGetters, mapMutations, mapActions } from \"vuex\";\nimport Paginate from \"vuejs-paginate\";\nimport Vue from \"vue\"; // Vue.component(\"paginate-v3\", Paginate);\n\nexport default {\n  data() {\n    return {\n      isOpen: false,\n      valueOfInput: \"10 bản ghi trên 1 trang\",\n      pagingData: [{\n        selected: true,\n        value: 10\n      }, {\n        selected: false,\n        value: 20\n      }, {\n        selected: false,\n        value: 30\n      }, {\n        selected: false,\n        value: 50\n      }, {\n        selected: false,\n        value: 100\n      }],\n      prevText: \"Trước\",\n      nextText: \"Sau\"\n    };\n  },\n\n  computed: { ...mapGetters([\"totalNumberEmployee\"]),\n    ...mapGetters([\"totalPages\"]),\n    ...mapGetters([\"pageIndex\"]),\n    ...mapGetters([\"pageSize\"])\n  },\n  watch: {\n    /**\r\n     * Reassign value of input\r\n     * Author: PhongVQ (11/02/2022)\r\n     */\n    pageSize: function (value) {\n      this.valueOfInput = `${value} bản ghi trên 1 trang`;\n    }\n  },\n  methods: {\n    /**\r\n     * Event choose ...Records per page\r\n     * Change PageSize\r\n     * Author: PhongVQ (11/02/2022)\r\n     */\n    clickOption(option) {\n      this.changePageSize(option.value); // Reset to page 1\n\n      this.changePageIndex(1);\n      this.pagingData.forEach(data => {\n        data.selected = false;\n      });\n      option.selected = true;\n      this.isOpen = false;\n    },\n\n    /**\r\n     * Alter page number\r\n     * Change PageIndex\r\n     * Author: PhongVQ (11/02/2022)\r\n     */\n    eventPaging(pageNum) {\n      this.changePageIndex(pageNum);\n    },\n\n    ...mapMutations([\"setPageIndex\"]),\n    ...mapActions([\"changePageIndex\"]),\n    ...mapMutations([\"setPageSize\"]),\n    ...mapActions([\"changePageSize\"])\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAASA,UAAT,EAAqBC,YAArB,EAAmCC,UAAnC,QAAqD,MAArD;AAEA,OAAOC,QAAP,MAAqB,gBAArB;AACA,OAAOC,GAAP,MAAgB,KAAhB