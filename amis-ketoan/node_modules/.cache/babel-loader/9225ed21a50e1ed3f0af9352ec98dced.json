{"ast":null,"code":"import axios from \"axios\";\nimport EmployeeDetail from './EmployeeDetail.vue';\nexport default {\n  name: \"EmployeeList\",\n  components: {\n    EmployeeDetail\n  },\n\n  created() {\n    try {\n      var me = this;\n      axios.get(\"https://amis.manhnv.net/api/v1/Employees\").then(response => {\n        // console.log(response.data)})\n        me.employees = response.data;\n      });\n    } catch (error) {\n      console.log(error);\n    }\n  },\n\n  mounted() {\n    this.formatDate();\n  },\n\n  methods: {\n    btnAddOnclick() {\n      this.isShowDialog = true;\n    },\n\n    showEmployeeDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    formatDate(dateOfBirth) {\n      if (dateOfBirth) {\n        // chuyển từ dạng string sang dạng Date\n        dateOfBirth = new Date(dateOfBirth); // lấy ngày\n\n        let date = dateOfBirth.getDate(); // lấy tháng\n\n        let month = dateOfBirth.getMonth() + 1; // lấy năm\n\n        let year = dateOfBirth.getFullYear(); // thêm số 0 vào trước nếu chỉ có một kí tự\n\n        date = date < 10 ? `0${date}` : date;\n        month = month < 10 ? `0${month}` : month;\n        return `${date}/${month}/${year}`;\n      } else {\n        return \"\";\n      }\n    }\n\n  },\n\n  data() {\n    return {\n      employees: null,\n      isShowDialog: false\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4HA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,cAAP,MAA2B,sBAA3B;AACA,eAAe;EACbC,IAAI,EAAE,cADO;EAEbC,UAAU,EAAE;IACRF;EADQ,CAFC;;EAKbG,OAAO,GAAG;IACR,IAAI;MACF,IAAIC,EAAC,GAAI,IAAT;MACAL,KAAK,CAACM,GAAN,CAAU,0CAAV,EAAsDC,IAAtD,CAA4DC,QAAD,IAAc;QACvE;QACAH,EAAE,CAACI,SAAH,GAAeD,QAAQ,CAACE,IAAxB;MACD,CAHD;IAIF,CANA,CAME,OAAOC,KAAP,EAAc;MACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;IACF;EACD,CAfY;;EAgBbG,OAAO,GAAG;IACR,KAAKC,UAAL;EACD,CAlBY;;EAmBbC,OAAO,EAAE;IACPC,aAAa,GAAE;MACb,KAAKC,YAAL,GAAkB,IAAlB;IACD,CAHM;;IAKPC,kBAAkB,CAACC,MAAD,EAAQ;MACtB,KAAKF,YAAL,GAAoBE,MAApB;IACH,CAPM;;IASPL,UAAU,CAACM,WAAD,EAAc;MACtB,IAAIA,